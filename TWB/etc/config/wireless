
config wifi-device 'wifi0'
	option type 'qcawifi'
	option hwmode '11ng'
	option disabled '0'
	option repacd_map_bsta_preference '1'
	option channel '6'
	option htmode 'HT40'
	option dbdc_enable '0'
	option repacd_map_bsta_selected '0'
	option country 'IN'
	option txchainmask '3'
	option rxchainmask '3'
	

config wifi-device 'wifi1'
	option type 'qcawifi'
	option hwmode '11ac'
	option disabled '0'
	option repacd_map_bsta_preference '2'
	option channel '36'
	option htmode 'HT80'
	option dbdc_enable '0'
	option country 'IN'
	option txchainmask '3'
	option rxchainmask '3'

config wifi-iface
	option device 'wifi0'
	option network 'lan'
	option wps_pbc '1'
	option wps_pbc_enable '0'
	option wps_pbc_start_time '0'
	option wps_pbc_duration '120'
	option encryption 'psk2+ccmp'
	option mode 'sta'
	option map '1'
	option MapBSSType '128'
	option wps_state '1'
	option wps_pbc_skip_connected_sta '1'
	option wds '1'

config wifi-iface
	option device 'wifi0'
	option network 'lan'
	option wps_pbc '1'
	option wps_pbc_enable '0'
	option wps_pbc_start_time '0'
	option wps_pbc_duration '120'
	option encryption 'psk2+ccmp'
	option mode 'ap'
	option map '1'
	option MapBSSType '96'
	option multi_cred '0'
	option wds '1'
	option rrm '1'

config wifi-iface
	option device 'wifi1'
	option network 'lan'
	option wps_pbc '1'
	option wps_pbc_enable '0'
	option wps_pbc_start_time '0'
	option wps_pbc_duration '120'
	option encryption 'psk2+ccmp'
	option mode 'ap'
	option map '1'
	option MapBSSType '96'
	option multi_cred '0'
	option wds '1'
	option rrm '1'

